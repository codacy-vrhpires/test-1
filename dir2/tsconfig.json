{
    "include": ["node_modules/cypress", "cypress/**/*.ts"],
    "compilerOptions": {
        "baseUrl": "./",
        "types": ["cypress", "cypress-file-upload", "cypress-wait-until", "cy-verify-downloads", "cypress-real-events", "cypress-plugin-api", "@mocks-server/cypress-commands"],
        "lib": ["es2022", "dom"],
        "target": "es5",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "paths": {
            "@fixtures/*": ["cypress/fixtures/*"]
        }
    }
}